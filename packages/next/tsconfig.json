    "allowJs": true,
    "noEmit": true,
    "module": "esnext",
    "target": "ES2017",
    "esModuleInterop": true,
    "jsx": "react",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": "../..",
    "paths": {
      "@babel/types": [
        "babel-types/index.d.ts",
        "@types/babel-types/index.d.ts",
        "node_modules/@types/babel-types/index.d.ts",
