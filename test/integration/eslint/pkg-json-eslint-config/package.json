{
  "name": "hello-world",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "plugin:@next/next/recommended"
    ],
    "rules": {
      "@next/next/no-sync-scripts": "off",
      "@next/next/no-css-tags": "warn",
      "@next/next/no-html-link-for-pages": [
        "error",
        "./"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    }
  }
}
