    "node-fetch": "1.6.3",
    "node-notifier": "5.1.2",
    "nyc": "10.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "standard": "9.0.2",
    "wd": "1.1.3"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ]
  }
