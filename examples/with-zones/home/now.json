{
    "version": 2,
    "name": "with-zones-home",
    "alias": "with-zones.nextjs.org",
    "build": {
        "env": {
            "DEPLOY": "true"
        }
    },
    "builds": [
        { "src": "package.json", "use": "@now/next" }
    ],
    "routes": [
        { "src": "/blog", "dest": "https://with-zones-blog.nextjs.org" }
    ]
}